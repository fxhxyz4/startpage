{
	"name": "startpage",
	"version": "1.0.0",
	"description": "A modern start page",
	"keywords": [
		"sass",
		"startpage",
		"animatecss",
		"javascript",
		"bootstrap"
	],
	"author": "fxhxyz <fxhxyz@proton.me> (https://fxhxyz.vercel.app/)",
	"homepage": "https://fxhxyz.vercel.app/",
	"bugs": "https://github.com/fxhxyz4/startpage/issues",
	"license": "MIT",
	"scripts": {
		"dev": "parcel ./src/index.html --dist-dir dist --cache-dir ./src/cache --no-source-maps -p 1010",
		"watch": "parcel watch ./src/index.html --dist-dir dist --cache-dir ./src/cache --no-source-maps",
		"build": "parcel build ./src/index.html --public-url /startpage/ --dist-dir build"
	},
	"devDependencies": {
		"@parcel/packager-raw-url": "^2.8.3",
		"@parcel/transformer-sass": "2.8.3",
		"@parcel/transformer-webmanifest": "^2.8.3",
		"parcel": "^2.8.3"
	},
	"dependencies": {
		"handlebars": "^4.7.7"
	}
}
