{
	"name": "StartPage",
	"version": "1.0.0",
	"description": "Modern start-page",
	"keywords": [
		"start-page",
		"scss",
		"homepage",
		"parcel",
		"posthtml"
	],
	"type": "module",
	"author": {
		"name": "fxhxyz4",
		"email": "fxhxyz@proton.me",
		"url": "https://fxhxyz.vercel.app/"
	},
	"homepage": "https://fxhxyz4.github.io/startpage/",
	"bugs": "https://github.com/fxhxyz4/startpage/issues",
	"repository": {
		"type": "git",
		"url": "git://github.com/fxhxyz4/startpage.git"
	},
	"maintainers": [
		{
			"name": "fxhxyz4",
			"email": "fxhxyz@proton.me"
		}
	],
	"license": "ISC",
	"scripts": {
		"dev": "parcel --no-content-hash ./src/index.html --dist-dir dist --cache-dir ./src/cache --no-source-maps -p 1324",
		"watch": "parcel watch --no-content-hash ./src/index.html --dist-dir dist --cache-dir ./src/cache --no-source-maps",
		"build": "parcel build --no-content-hash ./src/img/symbol-defs.svg --public-url ./ --cache-dir ./src/cache --no-source-maps --dist-dir build && parcel build --no-content-hash ./src/index.html --public-url ./ --cache-dir ./src/cache --no-source-maps --dist-dir build"
	},
	"devDependencies": {
		"@parcel/packager-raw-url": "^2.9.2",
		"@parcel/transformer-json": "^2.8.3",
		"@parcel/transformer-sass": "2.13.2",
		"@parcel/transformer-webmanifest": "^2.8.3",
		"parcel": "^2.9.2",
		"posthtml-include": "^1.7.4"
	},
	"engines": {
		"node": "18.x"
	}
}
