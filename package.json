	},
	"maintainers": [
		{
			"name": "fxhxyz4",
			"email": "fxhxyz@proton.me"
		}
	],
	"license": "ISC",
	"scripts": {
		"dev": "parcel ./src/index.html --dist-dir dist --cache-dir ./src/cache --no-source-maps -p 1324",
		"watch": "parcel watch ./src/index.html --dist-dir dist --cache-dir ./src/cache --no-source-maps",
		"build": "parcel build ./src/index.html --public-url /startpage/ --cache-dir ./src/cache --no-source-maps --dist-dir build",
		"deploy": "gh-pages -d build"
	},
	"devDependencies": {
		"@parcel/packager-raw-url": "^2.8.3",
		"@parcel/transformer-json": "^2.8.3",
		"@parcel/transformer-sass": "2.8.3",
		"@parcel/transformer-webmanifest": "^2.8.3",
		"parcel": "^2.8.3",
		"posthtml-include": "^1.7.4"
	},
	"dependencies": {
		"gh-pages": "^5.0.0"
	},
	"engines": {
		"node": "18.x"
	}
